package a2;

import java.awt.Color;

public abstract class GameObject {
	private Color myColor;
	private int locX, locY;
	
	public Color getColor() {
		return myColor;
	}
	
	public int getX() {
		return locX;
	}
	
	public int getY() {
		return locY;
	}
	
	public void setX(int x) {
		locX = x;
	}
	
	public void setY(int y) {
		locY = y;
	}
}
