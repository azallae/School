package a4;

import java.awt.Color;

import a2.LandscapeItem;

public class Rock extends LandscapeItem {
	private int width, height;
	
	public Rock(int x, int y, int w, int h) {
		width = w;
		height = h;
		myColor = Color.GRAY;
	}
	
	private int getWidth() {
		return width;
	}
	
	private int getHeight() {
		return height;
	}
	
	public String toString() {
		return "\nRock: Loc = " + getX() + ", " + getY() + "; Size = " + getWidth() + ", " + getHeight() + "; Color = " + getColor().toString();
	}
}
