package a2;

import java.awt.Color;

public abstract class GameObject {
	private Color myColor;
	private int locX, locY;
	
	public Color getColor() {
		return myColor;
	}
	
	public int getX() {
		return locX;
	}
	
	public int getY() {
		return locY;
	}
	
	public void setX(int x) {
		if (x >= 0 && x < 1024) {
			locX = x;
		}
	}
	
	public void setY(int y) {
		if (y >= 0 && y < 1024) {
			locY = y;
		}
	}
}
